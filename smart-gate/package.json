    "@latticexyz/store": "2.2.14",
    "eslint": "8.57.0",
    "mprocs": "^0.7.1",
    "shx": "^0.3.4",
    "typescript": "5.4.2"
  },
  "engines": {
    "node": "^18",
    "pnpm": "^8 || ^9"
  },
